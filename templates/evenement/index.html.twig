        <td>{{ e.contact }}</td>
        <td>{{ e.infos }}</td>
        <td>{{ e.type }}</td>
        <td>{{ e.circuit }}</td>
        <td><a href="{{ path ('delete',{'id':e.id}) }}">Supprimer</a></td>
        <td><a href="{{ path ('updateevent',{'id':e.id}) }}">Modifier</a></td>
    </tr {% endfor %}>

