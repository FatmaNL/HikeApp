<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>{% block title %}HIKEAPP{% endblock %}</title>

    {% block css %}
    <!-- Bootstrap core CSS -->
    <link href="{{ asset('lib/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
    <!--external css-->
    <link href="{{ asset('lib/font-awesome/css/font-awesome.css') }}" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="{{ asset('css/style.css') }}" rel="stylesheet">
    <link href="{{ asset('css/style-responsive.css') }}" rel="stylesheet">
    {% endblock %}
    {% block additional_css %}
    {% endblock %}
   </head>

<body>
<section id="container">
    <!-- **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
    <!--header start-->
    <header class="header black-bg">
        <div class="sidebar-toggle-box">
            <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
        </div>
        <!--logo start-->
        <a href="/" class="logo"><b>HIKE<span>APP</span></b></a>
        <!--logo end-->
        <div class="top-menu">
            <ul class="nav pull-right top-menu">
                <li>
                    <a class="logout" href="login.html">Logout</a>
                </li>
            </ul>
        </div>
    </header>
    <!--header end-->

    <!-- **********************************************************************************************************************************************************
           MAIN SIDEBAR MENU
           *********************************************************************************************************************************************************** -->
    <!--sidebar start-->
    <aside>
        <div id="sidebar" class="nav-collapse ">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu" id="nav-accordion">
                <p class="centered">
                    <a href="profile.html"><img src="img/ui-sam.jpg" class="img-circle" width="80"></a>
                </p>
                <h5 class="centered">Sam Soffes</h5>
                {% block dash %}
                <li class="mt">
                    <a href="index.html">
                        <i class="fa fa-dashboard"></i>
                        <span>Dashboard</span>
                    </a>
                </li>
                {% endblock %}

                <li class="sub-menu">
                    <a href="javascript:;">
                        <i class="fa fa-desktop"></i>
                        <span>Gestion des événements</span>
                    </a>
                    <ul class="sub">
                        {% block rando %}
                        <li>
                            <a href="general.html">Gestion des rondonnées</a>
                        </li>
                        {% endblock %}
                        {% block camp %}
                        <li>
                            <a href="buttons.html">Gestion des campings</a>
                        </li>
                        {% endblock %}
                    </ul>
                </li>
                {% block sent %}
                <li class="sub-menu">
                    <a href="javascript:;">
                        <i class="fa fa-cogs"></i>
                        <span>Gestion des sentiers</span>
                    </a>
                </li>
                {% endblock %}
                {% block transpo %}
                <li class="sub-menu">
                    <a href="javascript:;">
                        <i class="fa fa-book"></i>
                        <span>Gestion des transports</span>
                    </a>
                </li>
                {% endblock %}
                {% block gestprod %}
                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="fa fa-desktop"></i>
                            <span>Gestion gamme de produits</span>
                        </a>
                        <ul class="sub">
                            <li>
                                <a href="{{ path('afficheProduit') }}">Gestion des produits</a>
                            </li>
                            <li>
                                <a href="{{ path('categorie') }}">Gestion des categories</a>
                            </li>
                        </ul>
                    </li>
                {% endblock %}
                {% block blog %}
                <li>
                    <a href="inbox.html">
                        <i class="fa fa-envelope"></i>
                        <span>Gestion des blogs</span>
                    </a>
                </li>
                {% endblock %}
            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->
    {% block body %}
    {% endblock %}
<!--footer start-->
<footer class="site-footer">
    <div class="text-center">
        <p>
            &copy; Copyrights <strong>HIKEAPP</strong>. All Rights Reserved
        </p>
        <div class="credits">
            Created by <strong>Esprit</strong> Geeks
        </div>
        <a href="basic_table.html#" class="go-top">
            <i class="fa fa-angle-up"></i>
        </a>
    </div>
</footer>
<!--footer end-->
</section>
{% block js %}
<!-- js placed at the end of the document so the pages load faster -->
<script src="{{ asset('lib/jquery/jquery.min.js') }}"></script>
<script src="{{ asset('lib/bootstrap/js/bootstrap.min.js') }}"></script>
<script class="include" type="text/javascript" src="{{ asset('lib/jquery.dcjqaccordion.2.7.js') }}"></script>
<script src="{{ asset('lib/jquery.scrollTo.min.js') }}"></script>
<script src="{{ asset('lib/jquery.nicescroll.js') }}" type="text/javascript"></script>
<!--common script for all pages-->
<script src="{{ asset('lib/common-scripts.js') }}"></script>
<!--script for this page-->
{% endblock %}
</body>

</html>